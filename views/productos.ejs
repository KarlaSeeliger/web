
<link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/css/bootstrap.min.css"
    integrity="sha384-Vkoo8x4CGsO3+Hhxv8T/Q5PaXtkKtu6ug5TOeNV6gBiFeWPGFN9MuhOf23Q9Ifjh" crossorigin="anonymous">

<nav class="navbar navbar-expand-lg navbar-light " style="background-color:rgb(189, 189, 85)">
    <a class="navbar-brand" href="/home">Mitienda
    </a>
    <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNav"
        aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
        <span class="navbar-toggler-icon"></span>
    </button>
    <div class="collapse navbar-collapse" id="navbarNav">
        <ul class="navbar-nav">
            <li class="nav-item ">
                <a class="nav-link" href="/productos">Productos <span class="sr-only">(current)</span></a>
            </li>
          
        </ul>
    </div>
</nav>




<div class="container">
    <h1>Productos</h1>
    <div class="row text-center" style="display:flex; flex-wrap: wrap;">
        <% productos.forEach(function(producto){%>
        <div class="col-md-3 d-flex">
            <div class="card" style="width: 20rem;">
                <img src="<%= producto.image %>" class="card-img-top" alt="20">
                <div class="card-body flex-fill">
                    <h5 class="card-title"><%= producto.nombre %>
</h5>
                    <p class="card-text">Some quick example text to build on the card title and make up the bulk of the card's
                        content.</p>
                    <a href="#" class="btn btn-primary">Go somewhere</a>
                </div>
            </div>
        
            
        </div>
            <%})%>
    </div>

    </div>


